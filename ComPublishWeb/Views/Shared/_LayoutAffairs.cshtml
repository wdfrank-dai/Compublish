@using Telerik.Web.Mvc.UI
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>@ViewBag.Title</title>
     @*@Assets.CSS("base.css")*@
    <link href="@Url.Content("~/Content/SiteAffaris.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/basel.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/baseJAffaris.css")" rel="stylesheet" type="text/css" /> 
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plusIns/multiselect/css/style.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plusIns/multiselect/css/prettify.css")" /> 
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" /> 
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/js/jquery-ui.css")"/>
    @*<script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>*@
        @(Html.Telerik().StyleSheetRegistrar()
                    .DefaultGroup(group => group
                    .Add("telerik.common.css")
                    .Add("telerik.windows7.min.css")
                    .Compress(true))
                    )
</head>

<body>

<div id="container">

           <div id="navigation">
                 @Html.Partial("MainNavigation")
          </div>

        <div id="header">
            <div id="system_name"></div>
            <div id="U_name"></div>

        </div>
        <div id="wrapper" class="wat-cf">
            <div id="main">
                @RenderBody()                
            </div>            
        </div>
        @Html.Partial("_Footer")
    </div>


    <div class="page">
        
    @(Html.Telerik().ScriptRegistrar())
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.22.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/json2.js")" type="text/javascript"></script>  
       
    @RenderSection("Footer", false)
    </div>
</body>
</html>
