@model IEnumerable<WidgetDateLibrary.Models.WebPageUnits>
@{
    Layout = null;
}
<div id="sidebar">
@{if (Model.Count() != 0) {
    <div class="block">

  

    @foreach (var m in Model)
    {
        <h3 class="h3title">@m.showtitle</h3>
        <div class="content">
        <ul>
            <li>  
            @Html.GetPageContent(m.pageunitid, null)
            </li>
        </ul>
       </div>
    }
    </div>  
    }
    }
</div>
